{
  "id": null,
  "uid": "nextspyke-overview",
  "title": "NextSpyke Overview",
  "tags": ["nextspyke", "app"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "stat",
      "title": "App Up",
      "id": 1,
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "app_up",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false },
        "orientation": "horizontal",
        "textMode": "auto"
      }
    },
    {
      "type": "stat",
      "title": "Iterations Total",
      "id": 2,
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "targets": [
        {
          "expr": "app_iterations_total",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false }
      }
    },
    {
      "type": "stat",
      "title": "Failures Total",
      "id": 3,
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "app_iteration_failures_total",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false }
      }
    },
    {
      "type": "stat",
      "title": "Last Iteration (s ago)",
      "id": 4,
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "targets": [
        {
          "expr": "time() - app_last_iteration_timestamp_seconds",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "values": false }
      }
    },
    {
      "type": "timeseries",
      "title": "Iteration Duration (p50/p95/p99)",
      "id": 5,
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 4 },
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum(rate(app_iteration_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(app_iteration_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(app_iteration_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Failures by Reason",
      "id": 6,
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 12 },
      "targets": [
        {
          "expr": "sum by (reason) (rate(app_iteration_failure_reasons_total[5m]))",
          "legendFormat": "{{reason}}",
          "refId": "A"
        }
      ]
    }
  ]
}
